{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base","prettier","plugin:node/recommended"],
    "plugins":["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prefer-arrow-callback":"off",
        "func-names":"off",
        "consistent-return":"off",
        "no-underscore-dangle":"off",
        "no-param-reassign":"off",
        "no-process-exit":"off"
    }
}
